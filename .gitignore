# Adjusted to work when .gitignore is placed in the root directory "lucina"
# and the Unity project is inside "LucinaGame/"

# Unity-generated folders
/LucinaGame/[Ll]ibrary/
/LucinaGame/[Tt]emp/
/LucinaGame/[Oo]bj/
/LucinaGame/[Bb]uild/
/LucinaGame/[Bb]uilds/
/LucinaGame/[Ll]ogs/
/LucinaGame/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/LucinaGame/[Mm]emoryCaptures/

/LucinaGame/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /LucinaGame/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/LucinaGame/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/LucinaGame/.vs/

/LucinaGame/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/LucinaGame/.ExportedObj/
/LucinaGame/.consulo/
/LucinaGame/*.csproj
/LucinaGame/*.unityproj
/LucinaGame/*.sln
/LucinaGame/*.suo
/LucinaGame/*.tmp
/LucinaGame/*.user
/LucinaGame/*.userprefs
/LucinaGame/*.pidb
/LucinaGame/*.booproj
/LucinaGame/*.svd
/LucinaGame/*.pdb
/LucinaGame/*.mdb
/LucinaGame/*.opendb
/LucinaGame/*.VC.db

# Unity3D generated meta files
/LucinaGame/*.pidb.meta
/LucinaGame/*.pdb.meta
/LucinaGame/*.mdb.meta

# Unity3D generated file on crash reports
/LucinaGame/sysinfo.txt

# Builds
/LucinaGame/*.apk
/LucinaGame/*.aab
/LucinaGame/*.unitypackage
/LucinaGame/*.unitypackage.meta
/LucinaGame/*.app

# Crashlytics generated file
/LucinaGame/.crashlytics-build.properties

# Packed Addressables
/LucinaGame/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/LucinaGame/[Aa]ssets/[Ss]treamingAssets/aa.meta
/LucinaGame/[Aa]ssets/[Ss]treamingAssets/aa/*

# Asset store
/LucinaGame/[Aa]ssets/ASSETSTORE/*
